<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:my="urn:import:org.tagaprice.client.gwt.client.generics.widgets" 
	>
	<g:VerticalPanel>
		<g:HTML>
			<h1>Edit / Add receipt</h1>
		</g:HTML>
		<g:DecoratorPanel>
			<g:HTMLPanel>
				<table>
					<tr>
						<td>
							<g:Label ui:field="nameI18N">Name</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="name"></g:TextBox>
						</td>
					</tr>	
					<tr>
						<td>
							<g:Label ui:field="dateI18N">date</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="date"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td>
							<g:Label ui:field="shopI18N">Shop</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="shop"></g:TextBox>
						</td>
					</tr>
					<tr>
						<td colspan="2">
							<g:Label>Products</g:Label>
						</td>
					</tr>	
					<tr>
						<td>
							<g:Label ui:field="searchProductI18N">searchProduct</g:Label>
						</td>
						<td>
							<g:TextBox ui:field="searchProducts"></g:TextBox>
						</td>
					</tr>			
					<tr>
						<td colspan="2">
							<my:ReceiptEntrySelecter ui:field="receiptEntrySelecter" />
						</td>
					</tr>
					<tr>
						<td colspan="2">
							<g:Button ui:field="cancelButton">cancel</g:Button>
							<g:Button ui:field="saveButton">save product</g:Button>
						</td>
					</tr>
				</table>
			</g:HTMLPanel>

		</g:DecoratorPanel>
	</g:VerticalPanel>
</ui:UiBinder>